<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <fragment
        android:id="@+id/backConfigFragment_outl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.hifitoy.activities.filters.filter_fragment.BackConfigFragment"/>
    <fragment
        android:id="@+id/filterImportFragment_outl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.hifitoy.activities.filters.import_fragment.FilterImportFragment"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <fragment
            android:id="@+id/filtersFragment_outl"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:name="com.hifitoy.activities.filters.filter_fragment.FiltersFragment"/>
        <fragment
            android:id="@+id/biquadConfigFragment_outl"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:name="com.hifitoy.activities.filters.config_fragment.BiquadConfigFragment"/>
    </LinearLayout>

</FrameLayout>